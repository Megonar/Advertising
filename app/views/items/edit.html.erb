<h2>Edit new advertising</h2>
<h3><%= @item.owner %></h3>
<%= form_tag item_path(@item), method: :put do %>
    <%= label_tag :owner %>
    <%= text_field_tag :owner, @item.owner %>
    <%= label_tag :name %>
    <%= text_field_tag :name, @item.name %>
    <%= label_tag :description %>
    <%= text_field_tag :description, @item.description %>
    <%= submit_tag "Update" %>
    <% end %>

