<h2>Create new advertising</h2>

<%= form_tag items_path, method: :post do %>
    <%= label_tag :owner %>
    <%= text_field_tag :owner %>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
    <%= label_tag :description %>
    <%= text_field_tag :description %>
    <%= submit_tag "Save" %>
    <% end %>

